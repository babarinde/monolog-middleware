{
  "name": "oras/monolog-middleware",
  "description": "PSR-7 Monolog Middleware",
  "license": "MIT",
  "authors": [
    {
      "name": "Oras Al-Kubaisi",
      "email": "code@oras.me"
    }
  ],
  "require": {
    "php": "^7.2",
    "monolog/monolog": "^2.0",
    "psr/http-message": "~1.0.0",
    "symfony/http-foundation": "^3.0",
    "guzzlehttp/psr7": "^1.3 || ^1.4",
    "psr/http-server-middleware": "^1.0",
    "psr/container": "^1.0",
    "laminas/laminas-stratigility": "^3.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.3",
    "phpunit/phpunit": "^7.0",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "MonologMiddleware\\": "src/MonologMiddleware/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MonologMiddleware\\": "src/MonologMiddleware/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1-dev"
    }
  }
}
